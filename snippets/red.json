{
    "Red [Title]": {
        "prefix": "red [title]",
        "scope": "red",
        "body": [
            "Red [",
            "    Title: \"$1\"",
            "]"
        ],
        "description": "Red [ Title ]"
    },
    "Red [Title NeedsView]": {
        "prefix": "red [title needsview]",
        "scope": "red",
        "body": [
            "Red [",
            "    Title: \"$1\"",
            "    Needs: 'View",
            "]"
        ],
        "description": "Red [ Title NeedsView ]"
    },
    "either [][]": {
        "prefix": "either [][]",
        "scope": "red",
        "body": [
            "either ${1:condition} [",
            "    $2",
            "][",
            "    $3",
            "]"
        ],
        "description": "either condition [ ][ ]"
    },
    "forall []": {
        "prefix": "forall []",
        "scope": "red",
        "body": [
            "forall ${1:series} [",
            "    $2",
            "]"
        ],
        "description": "forall series [ ]"
    },
    "foreach []": {
        "prefix": "foreach []",
        "scope": "red",
        "body": [
            "foreach ${1:iteration} ${2:series} [",
            "    $3",
            "]"
        ],
        "description": "foreach iteration series [ ]"
    },
    "func []": {
        "prefix": "func []",
        "scope": "red",
        "body": [
            "func [${1:arg}][",
            "    $2",
            "]"
        ],
        "description": "func [ arg1 arg2 argN ][ ]"
    },
    "function []": {
        "prefix": "function []",
        "scope": "red",
        "body": [
            "function [${1: arg}][",
            "    $2",
            "]"
        ],
        "description": "function [ arg1 arg2 argN ][ ]"
    },
    "while []": {
        "prefix": "while []",
        "scope": "red",
        "body": [
            "while [${1:condition}] [",
            "    $2",
            "]"
        ],
        "description": "while [ condition ] [ ]"
    }
} 